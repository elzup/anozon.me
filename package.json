{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"dev": "next",
		"build": "next build",
		"export": "yarn build && next export",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"lint": "eslint --ext .ts,.tsx src pages",
		"setenv": "node scripts/setup_env.js",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"analyze": "ANALYZE=true BUNDLE_ANALYZE=browser next build"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.32",
		"@fortawesome/free-brands-svg-icons": "5.15.1",
		"@fortawesome/free-solid-svg-icons": "5.15.1",
		"@fortawesome/react-fontawesome": "0.1.12",
		"@zeit/next-preact": "0.1.0",
		"css-doodle": "0.8.5",
		"eslint": "7.12.1",
		"next": "10.0.1",
		"preact": "10.5.5",
		"preact-ssr-prepass": "1.1.2",
		"react": "16.14.0",
		"react-dom": "16.14.0",
		"react-ga": "3.2.0",
		"react-lazyload": "3.1.0",
		"react-scrollchor": "6.0.0",
		"styled-components": "5.2.1"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "10.0.1",
		"@types/node": "14.14.6",
		"@types/react-dom": "16.9.9",
		"@types/react-lazyload": "3.0.0",
		"@types/styled-components": "5.1.4",
		"@zeit/next-typescript": "1.1.1",
		"babel-plugin-lodash": "3.3.4",
		"babel-plugin-styled-components": "1.11.1",
		"dotenv": "8.2.0",
		"eslint-config-nzap": "3.2.0",
		"husky": "4.3.0",
		"lint-staged": "10.5.1",
		"prettier": "2.1.2",
		"typescript": "4.0.5",
		"webpack": "4.44.2",
		"workbox-webpack-plugin": "5.1.4"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
