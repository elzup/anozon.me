{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"export": "yarn build && next export",
		"lint": "eslint --ext .ts,.tsx src",
		"setenv": "node scripts/setup_env.js",
