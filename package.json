{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"deploy": "yarn build && up production",
		"deploy:url": "up url production",
		"coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.15",
		"@fortawesome/free-brands-svg-icons": "^5.7.2",
		"@fortawesome/free-solid-svg-icons": "^5.7.2",
		"@fortawesome/react-fontawesome": "^0.1.4",
		"@zeit/next-bundle-analyzer": "^0.1.2",
		"@zeit/next-typescript": "^1.1.1",
		"next": "^8.0.3",
		"next-images": "^1.0.4",
		"query-string": "^6.2.0",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-ga": "^2.5.7",
		"react-image": "^2.0.0",
		"react-lazyload": "^2.5.0",
		"react-redux": "^6.0.1",
		"react-router-dom": "^4.3.1",
		"react-scripts": "^2.1.5",
		"react-scrollchor": "^6.0.0",
		"redux": "^4.0.1",
		"redux-persist": "^5.7.0",
		"redux-thunk": "^2.2.0",
		"styled-components": "^4.1.3",
		"typescript": "^3.3.3333",
		"typescript-fsa": "^3.0.0-beta-2",
		"typescript-fsa-reducers": "^1.2.0"
	},
	"devDependencies": {
		"@types/node": "^11.9.5",
		"@types/react-dom": "^16.8.2",
		"@types/react-lazyload": "^2.3.1",
		"@types/react-redux": "^7.0.1",
		"@types/react-router-dom": "^4.3.1",
		"@types/styled-components": "^4.1.10",
		"babel-plugin-lodash": "^3.3.4",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.4",
		"prettier": "^1.16.4",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-react": "^3.6.0"
	},
	"lint-staged": {
		"src/**/*.js": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
