{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"dev": "next",
		"build": "next build",
		"export": "yarn build && next export",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"lint": "eslint --ext .ts,.tsx src",
		"setenv": "node scripts/setup_env.js",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.27",
		"@fortawesome/free-brands-svg-icons": "5.12.1",
		"@fortawesome/free-solid-svg-icons": "5.12.1",
		"@fortawesome/react-fontawesome": "0.1.8",
		"@zeit/next-preact": "0.1.0",
		"css-doodle": "0.7.7",
		"eslint": "6.8.0",
		"next": "9.2.2",
		"preact": "10.3.2",
		"preact-ssr-prepass": "1.0.1",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-ga": "2.7.0",
		"react-lazyload": "2.6.5",
		"react-scrollchor": "6.0.0",
		"styled-components": "5.0.1"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "9.2.2",
		"@types/node": "13.7.2",
		"@types/react-dom": "16.9.5",
		"@types/react-lazyload": "2.6.0",
		"@types/styled-components": "4.4.3",
		"@zeit/next-typescript": "1.1.1",
		"babel-plugin-lodash": "3.3.4",
		"babel-plugin-styled-components": "1.10.7",
		"dotenv": "8.2.0",
		"eslint-config-nzap": "1.4.6",
		"husky": "4.2.3",
		"lint-staged": "10.0.7",
		"prettier": "1.19.1",
		"typescript": "3.7.5",
		"webpack": "4.41.6",
		"workbox-webpack-plugin": "5.0.0"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
