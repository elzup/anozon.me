{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"deploy": "yarn build && up production",
		"deploy:url": "up url production",
		"coveralls": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.25",
		"@fortawesome/free-brands-svg-icons": "5.11.2",
		"@fortawesome/free-solid-svg-icons": "5.11.2",
		"@fortawesome/react-fontawesome": "0.1.6",
		"@zeit/next-bundle-analyzer": "0.1.2",
		"@zeit/next-typescript": "1.1.1",
		"css-doodle": "0.7.3",
		"next": "8.1.0",
		"next-images": "1.2.0",
		"query-string": "6.8.3",
		"react": "16.10.2",
		"react-dom": "16.10.2",
		"react-ga": "2.7.0",
		"react-image": "2.2.1",
		"react-lazyload": "2.6.2",
		"react-redux": "7.1.1",
		"react-router-dom": "4.3.1",
		"react-scripts": "2.1.8",
		"react-scrollchor": "6.0.0",
		"redux": "4.0.4",
		"redux-persist": "5.10.0",
		"redux-thunk": "2.3.0",
		"styled-components": "4.4.0",
		"typescript": "3.6.4",
		"typescript-fsa": "3.0.0",
		"typescript-fsa-reducers": "1.2.1"
	},
	"devDependencies": {
		"@types/node": "11.9.6",
		"@types/react-dom": "16.9.1",
		"@types/react-lazyload": "2.5.0",
		"@types/react-redux": "7.1.4",
		"@types/react-router-dom": "5.1.0",
		"@types/styled-components": "4.1.19",
		"babel-plugin-lodash": "3.3.4",
		"husky": "3.0.9",
		"lint-staged": "9.4.2",
		"prettier": "1.18.2",
		"tslint-config-prettier": "1.18.0",
		"tslint-plugin-prettier": "2.0.1",
		"tslint-react": "3.6.0"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
