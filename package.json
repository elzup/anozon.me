{
	"name": "anozon-me",
	"version": "1.0.0",
	"private": true,
	"homepage": "https://anozon.me/",
	"scripts": {
		"dev": "next",
		"build": "next build",
		"export": "yarn build && next export",
		"gen:env": "cp templates/temp.env .env.development.local && cp templates/temp.env .env.production.local",
		"lint": "eslint --ext .ts,.tsx src pages",
		"setenv": "node scripts/setup_env.js",
		"precommit": "lint-staged",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"analyze": "ANALYZE=true BUNDLE_ANALYZE=browser next build"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "1.2.35",
		"@fortawesome/free-brands-svg-icons": "5.15.3",
		"@fortawesome/free-solid-svg-icons": "5.15.3",
		"@fortawesome/react-fontawesome": "0.1.14",
		"@zeit/next-preact": "0.1.0",
		"css-doodle": "0.17.3",
		"eslint": "7.26.0",
		"next": "10.2.0",
		"preact": "10.5.13",
		"preact-ssr-prepass": "1.1.3",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-ga": "3.3.0",
		"react-lazyload": "3.2.0",
		"react-scroll": "^1.8.2",
		"styled-components": "5.3.0"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "10.2.0",
		"@types/node": "15.3.0",
		"@types/react-dom": "17.0.5",
		"@types/react-lazyload": "3.1.0",
		"@types/styled-components": "5.1.9",
		"@zeit/next-typescript": "1.1.1",
		"babel-plugin-lodash": "3.3.4",
		"babel-plugin-styled-components": "1.12.0",
		"dotenv": "9.0.2",
		"eslint-config-nzap": "4.3.1",
		"husky": "6.0.0",
		"lint-staged": "11.0.0",
		"prettier": "2.3.0",
		"typescript": "4.2.4",
		"webpack": "5.37.1",
		"workbox-webpack-plugin": "6.1.5"
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"prettier --config ./.prettierrc --write",
			"git add"
		]
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
